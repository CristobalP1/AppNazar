{
  "name": "appnazar",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:development": "envsub --env-file env/.env.development app.template.json app.json && expo start",
    "start:production": "envsub --env-file env/.env.production app.template.json app.json && expo start",
    "build:development": "envsub --env-file env/.env.development app.template.json app.json && eas build -p android && expo start",
    "build:production": "envsub --env-file env/.env.production app.template.json app.json && eas build -p android && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.5.0",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "expo": "~49.0.8",
    "expo-constants": "~14.4.2",
    "expo-image-picker": "^14.3.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "env-cmd": "^10.1.0",
    "envsub": "^4.1.0"
  },
  "private": true
}
